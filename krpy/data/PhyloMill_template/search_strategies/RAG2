# RAG2 (recombination activating gene 2) nuclear

[RAG2]

database = nuccore
query = ("RAG-2"[Title] OR "RAG2"[Title] OR "recombination activating protein 2"[Title] OR "recombination activating gene 2"[Title]) NOT ("promoter region" OR "ribonuclease A G" OR "UNVERIFIED:"[Title])

